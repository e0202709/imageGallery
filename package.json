    "predeploy": "npm run build",
    "deploy": "gh-pages -b main -d build"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@testing-library/react": "^12.1.2",
    "prettier": "^2.8.3"
  },
  
  "homepage": "https://e0202709.github.io/imageGallery"
